{
  "name": "raman-bajpai-portfolio",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://ramanbajpai7.github.io/",
  "scripts": {
    "build": "node fetch.js && react-scripts build",
    "check-format": "prettier -c \"./**/*.{js,css,json,scss}\"",
    "predeploy": "npm run build",
    "deploy": "gh-pages -b master -d build",
    "eject": "react-scripts eject",
    "format": "prettier --write \"./**/*.{js,css,json,scss}\"",
    "start": "node fetch.js && react-scripts start",
    "test": "react-scripts test"
  },
  "browserslist": {
    "production": [
      ">0.3%",
      "not ie 11",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      ">0.3%",
      "not ie 11",
      "not dead",
      "not op_mini all",
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "dependencies": {
    "ajv": "^8.17.1",
    "class-variance-authority": "0.7.1",
    "clsx": "2.1.1",
    "colorthief": "^2.3.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "gh-pages": "^2.1.1",
    "jest-canvas-mock": "^2.3.1",
    "lottie-react": "^2.4.0",
    "lucide-react": "0.561.0",
    "prop-types": "latest",
    "react": "^16.10.2",
    "react-dom": "^16.10.2",
    "react-easy-emoji": "^1.8.1",
    "react-headroom": "^3.0.0",
    "react-reveal": "^1.2.2",
    "react-scripts": "^5.0.1",
    "react-test-renderer": "^16.10.2",
    "react-twitter-embed": "3.0.3",
    "sass": "^1.32.8",
    "tailwind-merge": "3.4.0",
    "tailwindcss-animate": "1.0.7"
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@types/node": "25.0.2",
    "@types/react": "19.2.7",
    "@types/react-dom": "19.2.3",
    "dotenv": "^8.2.0",
    "postcss": "8.5.6",
    "prettier": "^2.3.1",
    "tailwindcss": "4.1.18",
    "typescript": "5.9.3"
  }
}
